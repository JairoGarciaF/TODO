<script>
    const checkboxes = document.querySelectorAll('[id^="check"]');

    checkboxes.forEach((checkbox, index) => {
        checkbox.addEventListener('change', (event) => {
            const span = document.querySelectorAll('.whitespace-nowrap')[index];
            if (event.target.checked) {
                span.classList.add('line-through'); // Reemplaza 'line-through	' con el nombre de tu clase
            } else {
                span.classList.remove('line-through'); // Reemplaza 'line-through	' con el nombre de tu clase
            }
        });
    });
</script>

